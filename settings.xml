<?xml version="1.0" encoding="UTF-8"?>
<settings xmlns="http://maven.apache.org/SETTINGS/1.0.0"
          xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
          xsi:schemaLocation="http://maven.apache.org/SETTINGS/1.0.0 https://maven.apache.org/xsd/settings-1.0.0.xsd">

  <mirrors>
    <mirror>
      <id>internal-nexus</id>
      <name>Internal Nexus Public Mirror</name>
      <url>https://nexus.local.omghax.pro/repository/maven-public/</url>
      <mirrorOf>central,apache.org</mirrorOf>
    </mirror>
    <mirror>
      <id>nexus-all</id>
      <name>Nexus mirror for all repositories</name>
      <url>https://nexus.local.omghax.pro/repository/maven-public/</url>
      <mirrorOf>*,!local</mirrorOf>
    </mirror>
  </mirrors>

</settings>
